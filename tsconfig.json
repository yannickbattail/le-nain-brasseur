{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": true,
    "stripInternal": true
  },
  "lib": [
    "dom",
    "es6"
  ],
  "include": [
    "./model/*.ts",
    "./*.ts"
  ]
}
